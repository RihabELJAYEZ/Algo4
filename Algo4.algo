ALGORITHM Insertion_sort
VAR
    n,x,i,j,key : INTEGER;
    arr : ARRAY ARRAY_OF INTEGER[n];

BEGIN
    x := arr.lenght;
    FOR i FROM 1 TO  x  DO
        key := arr[i];
        j := i-1 ;
        WHILE (j>0 and arr[j] > key) DO
            arr[j+1] := arr[j];
            j := j-;
        END_WHILE
        arr[j+1] := key;
    END_FOR

END